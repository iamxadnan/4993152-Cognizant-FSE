<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="country" class="com.cognizant.spring_learn.Country">
        <property name="code" value="IN"/>
        <property name="name" value="India"/>
    </bean>
    <bean id="in" class="com.cognizant.spring_learn.Country">
        <property name="code" value="IN"/>
        <property name="name" value="India"/>
    </bean>
    <bean id="nl" class="com.cognizant.spring_learn.Country">
        <property name="code" value="nl"/>
        <property name="name" value="NetherLands"/>
    </bean>
    <bean id="us" class="com.cognizant.spring_learn.Country">
        <property name="code" value="us"/>
        <property name="name" value="United States"/>
    </bean>
</beans>
